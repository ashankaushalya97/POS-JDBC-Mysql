<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="dashboard" prefHeight="293.0" prefWidth="656.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DashboardController">
    <children>
        <Label layoutX="99.0" layoutY="44.0" text="Home">
            <font>
                <Font name="System Bold" size="19.0"/>
            </font>
        </Label>
        <ImageView fitHeight="48.0" fitWidth="61.0" layoutX="35.0" layoutY="26.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../img/1.png"/>
            </image>
        </ImageView>
        <ImageView fx:id="icnManageCustomer" fitHeight="116.0" fitWidth="104.0" layoutX="70.0" layoutY="101.0"
                   onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation"
                   onMouseExited="#playMouseExitAnimation" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../img/3.png"/>
            </image>
        </ImageView>
        <ImageView fx:id="icnManageItems" fitHeight="104.0" fitWidth="96.0" layoutX="233.0" layoutY="105.0"
                   onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation"
                   onMouseExited="#playMouseExitAnimation" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../img/4.png"/>
            </image>
        </ImageView>
        <ImageView fx:id="icnManageOrders" fitHeight="104.0" fitWidth="88.0" layoutX="375.0" layoutY="109.0"
                   onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation"
                   onMouseExited="#playMouseExitAnimation" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../img/2.png"/>
            </image>
        </ImageView>
        <Label fx:id="lblMenu" alignment="CENTER" contentDisplay="CENTER" layoutX="17.0" layoutY="222.0"
               prefHeight="24.0" prefWidth="625.0" text="Welcome" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="17.0"/>
            </font>
        </Label>
        <ImageView fx:id="icnSearchOrders" fitHeight="104.0" fitWidth="88.0" layoutX="504.0" layoutY="105.0"
                   onMouseClicked="#navigate" onMouseEntered="#playMouseEnterAnimation"
                   onMouseExited="#playMouseExitAnimation" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../img/6.png"/>
            </image>
        </ImageView>
        <Label fx:id="lblDescription" alignment="CENTER" contentDisplay="CENTER" layoutX="19.0" layoutY="246.0"
               prefHeight="20.0" prefWidth="625.0" text="Please select one of above main operations to proceed"
               textAlignment="CENTER">
            <font>
                <Font name="System Italic" size="14.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
